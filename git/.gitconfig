[http]
  lowSpeedLimit = 0
  lowSpeedTime = 999999
  proxy = 192.168.162.20:2080
[https]
  proxy = 192.168.162.20:2080
[user]
  name = starifly
  email = 2003879+starifly@users.noreply.github.com
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[color "branch"]
  current = green bold
  local = normal
  remote = blue
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = "red bold 52"
  newNormal = "green bold"
  newHighlight = "green bold 22"
[color "status"]
  added = green
  changed = yellow
  untracked = red
[core]
  editor = vim
    # pager = less -FRSX
    # excludesfile = ~/.gitignore
    # whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  autocrlf = input
[alias]
    # Stage
  a = add

    # Branch
  br = branch

    # bisect
  bsb = bisect bad
  bsg = bisect good
  bsr = bisect reset
  bss = bisect start

    # Commit
  c = commit
  cm = commit -m
  ca = commit -am
  amend = commit --amend

    # Clone
  cl = clone

    # Checkout
  co = checkout
  cb = checkout -b

    # Cherry-pick
  cp = cherry-pick

    # Diff
  df = diff
  dc = diff --cached
  last = diff HEAD^

  m = merge

    # Pull/Push
  pl = pull
  p = push

    # Reset
  unstage = reset HEAD
  uncommit = reset --soft HEAD^

    # Remote
  r = remote -v

    # Misc
  st = status
  lg = log --all --graph --pretty=format:'%C(cyan)%h%Creset -%C(yellow)%d%Creset %Cgreen(%cr) %Cblue<%an>%Creset %s' --abbrev-commit --date=relative
  oneline = log --oneline --graph --all
  l = log --graph --date=short
  changes = log --pretty=format:\"%h %cr %cn %Cgreen%s%Creset\" --name-status
  short = log --pretty=format:\"%h %cr %cn %Cgreen%s%Creset\"
  simple = log --pretty=format:\" * %s\"
  shortnocolor = log --pretty=format:\"%h %cr %cn %s\"
[rerere]
  enabled = true
[push]
  default = simple
  followTags = true
